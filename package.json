{
  "name": "netlify-aurora",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "npx prisma generate && nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{css,scss,sass,html,vue}\" --ignore-path .gitignore",
    "lint:prettier": "prettier --check .",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lintfix": "prettier --write --list-different . && npm run lint:js -- --fix && npm run lint:style -- --fix",
    "lintfix:trypescript": "eslint \"functions/**/*.ts\" --fix",
    "prepare": "husky install",
    "test": "jest",
    "net-dev": "npm run prisma-generate && netlify dev",
    "prisma-generate": "prisma generate",
    "cypress:open": "cypress open"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  },
  "lint-staged": {
    "*.vue": "eslint --cache"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "@prisma/client": "^4.0.0",
    "core-js": "^3.23.5",
    "express": "^4.18.1",
    "nuxt": "^2.15.8",
    "serverless-http": "^3.0.1",
    "typescript": "^4.7.4",
    "vue": "^2.7.7",
    "vue-server-renderer": "^2.7.7",
    "vue-template-compiler": "^2.7.7",
    "webpack": "^4.46.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.18.9",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@nuxtjs/eslint-config": "^10.0.0",
    "@nuxtjs/eslint-module": "^3.1.0",
    "@nuxtjs/stylelint-module": "^4.1.0",
    "@nuxtjs/tailwindcss": "^4.2.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/node": "^16.11.45",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "@vue/test-utils": "^1.3.0",
    "babel-core": "^7.0.0-bridge.0",
    "cypress": "^10.3.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-nuxt": "^3.2.0",
    "eslint-plugin-vue": "^9.2.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "netlify-plugin-cypress": "^2.2.0",
    "postcss": "^8.4.14",
    "postcss-html": "^1.5.0",
    "prettier": "^2.7.1",
    "prisma": "^4.0.0",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^26.0.0",
    "ts-jest": "^28.0.7"
  }
}
