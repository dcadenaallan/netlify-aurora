{
  "name": "netlify-aurora",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "npx prisma generate && nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{css,scss,sass,html,vue}\" --ignore-path .gitignore",
    "lint:prettier": "prettier --check .",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lintfix": "prettier --write --list-different . && npm run lint:js -- --fix && npm run lint:style -- --fix",
    "lintfix:trypescript": "eslint \"functions/**/*.ts\" --fix",
    "prepare": "husky install",
    "test": "jest",
    "net-dev": "npm run prisma-generate && netlify dev",
    "prisma-generate": "prisma generate"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  },
  "lint-staged": {
    "*.vue": "eslint --cache && prettier --check --ignore-unknown",
    "*.{css,scss,sass,html,vue}": "stylelint"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "@prisma/client": "^4.0.0",
    "@types/node": "^14.18.21",
    "core-js": "^3.19.3",
    "express": "^4.18.1",
    "nuxt": "^2.15.8",
    "serverless-http": "^3.0.1",
    "typescript": "^4.7.4",
    "vue": "^2.6.14",
    "vue-server-renderer": "^2.6.14",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^4.46.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.18.2",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@nuxtjs/eslint-config": "^10.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/stylelint-module": "^4.1.0",
    "@nuxtjs/tailwindcss": "^4.2.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.4",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "@vue/test-utils": "^1.3.0",
    "babel-core": "7.0.0-bridge.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^3.1.0",
    "eslint-plugin-vue": "^8.2.0",
    "husky": "^7.0.4",
    "jest": "^28.1.2",
    "lint-staged": "^12.1.2",
    "postcss": "^8.4.4",
    "postcss-html": "^1.3.0",
    "prettier": "^2.5.1",
    "prisma": "^4.0.0",
    "stylelint": "^14.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-vue": "^1.1.0",
    "stylelint-config-standard": "^24.0.0",
    "ts-jest": "^28.0.5"
  }
}
